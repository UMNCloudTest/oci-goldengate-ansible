{
  "extracts": [
    {
      "name": "{{ extract_name_prefix }}SRC1",
      "state": "started",
      "config": {
        "source": {
          "type": "oracle",
          "connection": {
            "host": "{{ source_db_host }}",
            "port": "{{ source_db_port | default(1521) }}",
            "serviceName": "{{ source_db_service }}"
          }
        },
        "targets": [
          {
            "file": "./dirdat/{{ trail_file_prefix }}",
            "format": "trail"
          }
        ],
        "description": "Extract for {{ source_db_service }} database",
        "credentials": {
          "alias": "{{ source_db_alias }}"
        },
        "parameters": [
          "TABLE SCHEMA1.CUSTOMER_DATA, COLEXC (SSN, CREDIT_CARD_NUMBER, EMAIL);",
          "TABLE SCHEMA1.EMPLOYEE_INFO, COLEXC (SALARY, SSN, PHONE_NUMBER);",
          "TABLE SCHEMA2.FINANCIAL_RECORDS, COLEXC (ACCOUNT_NUMBER, ROUTING_NUMBER);"
        ]
      },
      "trail_file": "{{ trail_file_prefix }}"
    },
    {
      "name": "{{ extract_name_prefix }}SRC2", 
      "state": "started",
      "config": {
        "source": {
          "type": "oracle",
          "connection": {
            "host": "{{ source_db_host_2 }}",
            "port": "{{ source_db_port_2 | default(1521) }}",
            "serviceName": "{{ source_db_service_2 }}"
          }
        },
        "targets": [
          {
            "file": "./dirdat/{{ trail_file_prefix_2 }}",
            "format": "trail"
          }
        ],
        "description": "Extract for {{ source_db_service_2 }} database",
        "credentials": {
          "alias": "{{ source_db_alias_2 }}"
        },
        "raw_config": "EXTRACT EXTSRC2\nUSERIDREP oracledb\nREMOTETRAIL ./dirdat/es\nTABLE SCHEMA2.ORDER_HISTORY, COLEXC (PAYMENT_METHOD, BILLING_ADDRESS);\nTABLE SCHEMA2.USER_PROFILES, COLEXC (PASSWORD_HASH, EMAIL);"
      },
      "trail_file": "{{ trail_file_prefix_2 }}"
    }
  ]
}